{
  "expo": {
    "name": "AnimeHub",
    "slug": "animehub",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/animeicon2.png",

    "scheme": "animehubmobile",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bernard001.animehub",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#0f172a"
      }
    },

    "web": {
      "favicon": "./assets/images/animeicon2.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main-app-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#0f172a"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "7fee4f3a-eb9c-4bf9-8d87-1b6ab2962157"
      }
    },

    "owner": "bernard001"
  }
}